<?xml version="1.0" encoding="utf-8"?>

<omdoc version="1.6"
       xmlns="http://omdoc.org/ns"
       xmlns:dc="http://purl.org/dc/elements/1.1/" 
       xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
       xmlns:odo="http://www.omdoc.org/ontology#">

<metadata>
  <meta property="dc:title">Definition of a class</meta>
  <meta property="dc:creator">Christoph Lange</meta>
</metadata>

<theory name="definition-class" xml:id="definition-class">
  <metadata>
    <link rel="odo:semWebBase" href="http://kwarc.info/projects/krextor/test#"/>
 </metadata>

<imports from="logics/owl.omdoc#owl"/>

<symbol name="Foo" xml:id="Foo.sym">
  <type>
    <OMOBJ xmlns="http://www.openmath.org/OpenMath">
      <OMS cd="owl" name="Class"/>
    </OMOBJ>
  </type>
</symbol>

<definition for="Foo" type="simple">
  <OMOBJ xmlns="http://www.openmath.org/OpenMath">
    <OMA>
      <OMS cd="owl" name="unionOf"/>
      <OMS cd="owl" name="InverseFunctionalProperty"/>
      <OMS cd="owl" name="TransitiveProperty"/>
    </OMA>
  </OMOBJ>
</definition>

</theory>
</omdoc>

