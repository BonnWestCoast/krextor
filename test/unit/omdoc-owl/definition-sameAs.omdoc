<?xml version="1.0" encoding="utf-8"?>

<omdoc version="1.6"
       xmlns="http://omdoc.org/ns"
       xmlns:dc="http://purl.org/dc/elements/1.1/" 
       xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
       xmlns:odo="http://www.omdoc.org/ontology#">

<metadata>
  <meta property="dc:title">Definition of an individual by equivalence</meta>
  <meta property="dc:creator">Christoph Lange</meta>
</metadata>

<theory name="definition-sameAs" xml:id="definition-sameAs">
  <metadata>
    <link rel="odo:semWebBase" href="http://kwarc.info/projects/krextor/test#"/>
 </metadata>

<imports from="logics/owl.omdoc#owl"/>

<!-- no type: assumed to be an individual -->
<symbol name="foo" xml:id="foo.sym"/>
<symbol name="bar" xml:id="bar.sym"/>

<definition for="foo" type="simple">
  <OMOBJ xmlns="http://www.openmath.org/OpenMath">
    <OMS cd="definition-sameAs" name="bar"/>
  </OMOBJ>
</definition>

</theory>
</omdoc>

