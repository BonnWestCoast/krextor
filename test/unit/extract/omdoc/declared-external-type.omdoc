<?xml version="1.0" encoding="utf-8"?>

<omdoc version="1.3"
       xmlns="http://omdoc.org/ns"
       xmlns:dc="http://purl.org/dc/elements/1.1/">

<metadata>
  <meta property="dc:title">Symbol declaration with a type</meta>
  <meta property="dc:creator">Christoph Lange</meta>
</metadata>

<theory name="symbol-type" xml:id="symbol-type">
  <imports from="logics/owl.omdoc#owl"/>

  <symbol name="foo" xml:id="foo.sym"/>

  <type xml:id="foo.type" for="foo">
    <OMOBJ xmlns="http://www.openmath.org/OpenMath">
      <OMS cd="owl" name="Class"/>
    </OMOBJ>
  </type>
</theory>
</omdoc>
